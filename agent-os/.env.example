# Azure OpenAI (recommended)
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_DEPLOYMENT=gpt-4o
# Nome do deployment de embeddings no Azure (deve ser igual ao nome no Portal).
# Se aparecer DeploymentNotFound 404, crie o deployment ou ajuste este valor.
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding

# API keys fallback (if not using Azure OpenAI)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Runtime: dev (reload) or prd
RUNTIME_ENV=dev

# PostgreSQL (used by Docker Compose; DB_HOST=agentos-db when running in compose)
DB_HOST=localhost
DB_PORT=5432
DB_USER=ai
DB_PASS=ai
DB_DATABASE=ai
